meta {
  name: Register User
  type: graphql
  seq: 1
}

post {
  url: http://localhost:4000/gql
  body: graphql
  auth: inherit
}

body:graphql {
  mutation RegisterUser {
    registerUser(
      input: {
        email: "bria@example.com"
        password: "mypassword"
        passwordConfirmation: "mypassword"
      }
    ) {
      result {
        id
        email
      }
      metadata {
        token
      }
    }
  }
  
}
